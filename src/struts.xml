<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.multipart.maxSize" value="9000000" />
	<package name="package1" extends="struts-default" namespace="/Admin/pages">
		<action name="admin_*" class="xflfk.wicresoft.action.AdminAction"
			method="{1}">
			<result name="login" type="chain">admin_statistical</result>
			<result name="bookUploadOK" type="chain">admin_showBook</result>
			<result name="upInvenOK" type="chain">sadmin_upInven</result>
			<result name="upInvenOK" type="chain">admin_inveAlarm</result>
			<result name="bookStoreOK" type="chain">admin_showBook</result>
			<result name="updateBookOK" type="chain">admin_showUpdateBook</result>
			<result name="delStortOK" type="chain">admin_showStort</result>
			<result name="upStortOK" type="chain">admin_showUpStort</result>
			<result name="delAuthorOK" type="chain">admin_showAuthor</result>
			<result name="upAuthorOK" type="chain">admin_showUpAuthor</result>
			<result name="authorUploadOK" type="chain">admin_showAuthor</result>
			<result name="delOrderOK" type="chain">admin_showOrder</result>
			<result name="deliveryOK" type="chain">admin_noDelivery</result>
			<result name="exportOrderOK" type="chain">admin_showOrder</result>
			<result name="settingOK" type="chain">admin_showSetting</result>
			<result name="delNotesOK" type="chain">admin_showNotes</result>
			<result name="updateNoteOK" type="chain">admin_showNoteInfo</result>
			<result name="addNotesOK" type="chain">admin_showNotes</result>
			<result name="onlogin">admin_Login.jsp</result>
			<result name="ok">admin_BookList.jsp</result>
			<result name="showAddBookOK">admin_AddBook.jsp</result>
			<result name="adminUploadOK">addAdmin.jsp</result>
			<result name="inveAlarmOK">InventoryAlarm.jsp</result>
			<result name="BookInfoOK">BookInfo.jsp</result>
			<result name="showUpInvenOK">admin_upBook.jsp</result>
			<result name="showUpdateBookOK">admin_updateBook.jsp</result>
			<result name="showStortOK">admin_StortList.jsp</result>
			<result name="showUpStortOK">admin_UpStort.jsp</result>
			<result name="addStortOK">admin_AddStort.jsp</result>
			<result name="showAuthorOK">admin_AuthorList.jsp</result>
			<result name="showAuthorInfoOK">admin_AuthorInfo.jsp</result>
			<result name="showUpAuthorOK">admin_UpdateAuthor.jsp</result>
			<result name="showOrderOK">admin_OrderList.jsp</result>
			<result name="showDetailOK">admin_DetailList.jsp</result>
			<result name="noPaymentOK">admin_NoPayment.jsp</result>
			<result name="noDeliveryOK">admin_NoDelivery.jsp</result>
			<result name="showSettingOK">setting.jsp</result>
			<result name="loginOutOK">admin_Login.jsp</result>
			<result name="showNotrsOK">admin_notes.jsp</result>
			<result name="showNoteInfoOK">admin_updateNotes.jsp</result>
			<result name="statisticalOK">admin_statistical.jsp</result>
		</action>
		<action name="download" class="xflfk.wicresoft.action.DownloadAction"
			method="execute">
			<!-- 对Action类中的文件路径参数设置初始值 -->
			<param name="downloadPath">${savePath}</param>
			<!-- 设置一个stream类型的result -->
			<result name="success" type="stream">
				<!-- 设置下载文件的输入流属性名 -->
				<param name="inputName">targetFile</param>
				<!-- 设置下载文件的文件类型，配置后可以直接在浏览器上浏览，省略则会弹出是保存文件还是打开文件信息的对话框 -->
				<!-- <param name="contentType">image/jpg</param> -->
				<!-- 设置下载文件的文件名 -->
				<param name="contentDisposition">attachment;filename=${filename}</param>
				<!-- 设置下载文件的缓冲 -->
				<param name="bufferSize">2048</param>
			</result>
		</action>
		<action name="account_*" class="xflfk.wicresoft.action.AccountAction" method="{1}">
			<result name="allUserOK">admin_userList.jsp</result>
			<result name="allAdminOK">admin_adminList.jsp</result>
			<result name="delUserOK" type="chain">account_allUser</result>
			<result name="resetPassOK" type="chain">account_allUser</result>
			<result name="delAdminOK" type="chain">account_allAdmin</result>
			<result name="resetAdminOK" type="chain">account_allAdmin</result>
		</action>
	</package>
	<package name="package2" extends="struts-default" namespace="/User/pages">
		<action name="user_*" class="xflfk.wicresoft.action.UserAction"
			method="{1}">
			<result name="loginOK" type="chain">user_homeContent</result>
			<result name="loginOutOK" type="chain">user_homeContent</result>
			<result name="addConsignessOK" type="chain">user_myConsigness</result>
			<result name="delConsignessOK" type="chain">user_myConsigness</result>
			<result name="updateConsignessOK" type="chain">user_updConsigness</result>
			<result name="changeStateOK" type="chain">user_shoppingCart</result>
			<result name="allChooseOK" type="chain">user_shoppingCart</result>
			<result name="delGoodsOK" type="chain">user_shoppingCart</result>
			<result name="cartToOrderOK" type="chain">user_myOrders</result>
			<result name="cancelOrderOK" type="chain">user_myOrders</result>
			<result name="homeContentOK">user_index.jsp</result>
			<result name="allBookOK">user_BookList.jsp</result>
			<result name="searchBookOK">user_searchBooks.jsp</result>
			<result name="allAuthorOK">user_AuthorList.jsp</result>
			<result name="authorToBooksOK">user_AutToBooks.jsp</result>
			<result name="stortToBooksOK">user_StoToBooks.jsp</result>
			<result name="autDetailOK">user_AutDetail.jsp</result>
			<result name="loginNO">user_Login.jsp</result>
			<result name="register">user_Register.jsp</result>
			<result name="ConsignessOK">myConsigness.jsp</result>
			<result name="ConsignessNO">user_Login.jsp</result>
			<result name="addConsignessNO">user_Login.jsp</result>
			<result name="updConsignessOK">updConsigness.jsp</result>
			<result name="addConsOK">addConsigness.jsp</result>
			<result name="shoppingCartOK">shoppingcart.jsp</result>
			<result name="shoppingCartNO">user_Login.jsp</result>
			<result name="addToCartNO">user_Login.jsp</result>
			<result name="changeConsNO">user_Login.jsp</result>
			<result name="myOrdersNO">user_Login.jsp</result>
			<result name="payDetailNO">user_Login.jsp</result>
			<result name="sendOrderNO">user_Login.jsp</result>
			<result name="sendOrderOK">user_completeOrders.jsp</result>
			<result name="payDetailOK">user_PayOrders.jsp</result>
			<result name="myOrdersOK">user_Orders.jsp</result>
			<result name="changeConsOK">user_OrderCons.jsp</result>
			<result name="ordDetailOK">user_OrdDetail.jsp</result>
			<result name="payOrdDetailOK">user_payOrderDetail.jsp</result>
			<result name="changeConsigessOK">isOK.jsp</result>
			<result name="addToCartOK1">isOK.jsp</result>
			<result name="addToCartOK2">isNO.jsp</result>
			<result name="cartToOrderNO">isNO.jsp</result>
			<result name="consToUserOK">isOK.jsp</result>
			<result name="payOrderOK">isOK.jsp</result>
		</action>
	</package>
</struts>