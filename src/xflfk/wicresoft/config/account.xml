<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="package3" extends="struts-default" namespace="/Admin/pages">
		<action name="account_*" class="xflfk.wicresoft.action.AccountAction"
			method="{1}">
			<result name="allUserOK">admin_userList.jsp</result>
			<result name="allAdminOK">admin_adminList.jsp</result>
			<result name="delUserOK" type="chain">account_allUser</result>
			<result name="resetPassOK" type="chain">account_allUser</result>
			<result name="delAdminOK" type="chain">account_allAdmin</result>
			<result name="resetAdminOK" type="chain">account_allAdmin</result>
		</action>
	</package>
</struts>