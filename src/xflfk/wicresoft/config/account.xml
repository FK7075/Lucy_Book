<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="package3" extends="struts-default" namespace="/Admin/pages">
	<interceptors>
		<interceptor name="loginfk" class="xflfk.wicresoft.factory.LoginIntercept"></interceptor>
	</interceptors>
		<action name="account_*" class="xflfk.wicresoft.action.AccountAction"
			method="{1}">
			<interceptor-ref name="defaultStack"></interceptor-ref>
			<interceptor-ref name="loginfk">
			</interceptor-ref>
			<result name="allUserOK">admin_userList.jsp</result>
			<result name="allAdminOK">admin_adminList.jsp</result>
			<result name="retrieveBookOK">admin_retrieveBook.jsp</result>
			<result name="retrieveAuthorOK">admin_retrieveAuthor.jsp</result>
			<result name="onlogin">admin_Login.jsp</result>
			<result name="a_newPasswordOK">admin_Login.jsp</result>
			<result name="delUserOK" type="chain">account_allUser</result>
			<result name="resetPassOK" type="chain">account_allUser</result>
			<result name="delAdminOK" type="chain">account_allAdmin</result>
			<result name="resetAdminOK" type="chain">account_allAdmin</result>
		</action>
	</package>
		<package name="package31" extends="struts-default" namespace="/User/pages">
		<action name="account_*" class="xflfk.wicresoft.action.AccountAction"
			method="{1}">
			<result name="setUserInfoOK" type="chain">account_zbUserInfo</result>
			<result name="setUserInfoNO">user_Login.jsp</result>
			<result name="newPasswordOK">user_Login.jsp</result>
			<result name="zbUserInfoOK">user_userInfo.jsp</result>
			<result name="zbNewPassOK">user_newpassword.jsp</result>
		</action>
	</package>
</struts>